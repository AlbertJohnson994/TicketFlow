{
  "name": "ticketflow-backend",
  "version": "2.0.0",
  "description": "TicketFlow Backend API - Event Management & Ticket Sales",
  "type": "module",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "seed": "node src/seeders/seed.js",
    "db:reset": "rm -f database.sqlite && npm run seed",
    "docker:build": "npm install && npm run build",
    "docker:start": "node src/app.js"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "qrcode": "^1.5.4",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "tickets",
    "events",
    "sales",
    "api",
    "backend"
  ],
  "author": "TicketFlow Team",
  "license": "MIT"
}
